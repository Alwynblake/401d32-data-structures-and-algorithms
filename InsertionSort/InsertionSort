// const Node = require('./lib/node.js');

class InsertionSort(arr) => {
  while (i = 0; i < arr.length; i++) {
      if (arr[i - 1] > arr[i]) {
        sortDone = false;
        let tempSpot = arr[i - 1];
        arr[i - 1] = arr[i];
        arr[i] = tempSpot;
        console.log(arr, 'Index:', i);
      }
    }
};
  return arr;
